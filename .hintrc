{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scroll-behavior"
        ]
      }
    ],
    "axe/name-role-value": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "disown-opener": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "no-inline-styles": "off",
    "detect-css-reflows/layout": "off",
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/paint": "off"
  }
}